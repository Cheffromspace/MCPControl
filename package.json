{
  "name": "@modelcontextprotocol/control",
  "version": "1.0.0",
  "description": "Cross-platform control server for the Model Context Protocol",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "scripts"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modelcontextprotocol/control.git"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "automation",
    "control"
  ],
  "scripts": {
    "build": "node scripts/build-all.js",
    "build:ts": "tsc",
    "start": "node build/index.js",
    "dev": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.1",
    "@nut-tree/libnut": "^2.0.0",
    "clipboardy": "^4.0.0",
    "express": "^4.18.2",
    "jimp": "^1.6.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.9",
    "@vitest/coverage-v8": "^3.0.8",
    "audit-ci": "^6.6.1",
    "typescript": "^5.3.3",
    "vitest": "^3.0.8"
  }
}
